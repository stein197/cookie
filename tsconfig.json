{
	"compilerOptions": {
		"outDir": "out",
		"target": "ESNext",
		"strict": true,
		"strictNullChecks": false,
		"baseUrl": "src",
		"rootDir": "src",
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"allowUnusedLabels": false,
		"moduleResolution": "node",
		"declaration": true,
		"declarationDir": "types",
		"lib": [
			"DOM",
			"ES2015"
		]
	},
	"include": [
		"src"
	]
}
