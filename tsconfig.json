{
	"compilerOptions": {
		"outFile": "out/cookie.js",
		"target": "ES3",
		"strict": true,
		"strictNullChecks": false,
		"baseUrl": "src",
		"rootDir": "src",
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"allowUnusedLabels": false,
		"moduleResolution": "node",
		"downlevelIteration": true,
		"lib": [
			"DOM",
			"ES2015"
		]
	},
	"include": [
		"src"
	]
}
